  {
    "id": "crisp_message",
    "utilityProvider": "crisp",
    "openapiSpecification": {
      "openapi": "3.0.0",
      "info": {
        "title": "Crisp Message API",
        "version": "1.0.0",
        "description": "API to send messages to Crisp conversations"
      },
      "servers": [
        {
          "url": "https://api.crisp.chat"
        }
      ],
      "paths": {
        "/v1/website/{website_id}/conversation/{session_id}/message": {
          "post": {
            "operationId": "sendCrispMessage",
            "summary": "Send a message to a Crisp conversation",
            "parameters": [
              {
                "name": "website_id",
                "in": "path",
                "required": true,
                "schema": {
                  "type": "string"
                },
                "description": "The Crisp website ID"
              },
              {
                "name": "session_id",
                "in": "path",
                "required": true,
                "schema": {
                  "type": "string"
                },
                "description": "The Crisp session ID"
              }
            ],
            "requestBody": {
              "required": true,
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "required": [
                      "type",
                      "from",
                      "origin",
                      "content"
                    ],
                    "properties": {
                      "type": {
                        "type": "string",
                        "enum": [
                          "text"
                        ],
                        "default": "text"
                      },
                      "from": {
                        "type": "string",
                        "enum": [
                          "operator"
                        ],
                        "default": "operator"
                      },
                      "origin": {
                        "type": "string",
                        "enum": [
                          "chat"
                        ],
                        "default": "chat"
                      },
                      "content": {
                        "type": "string",
                        "description": "The message content"
                      }
                    }
                  }
                }
              }
            },
            "responses": {
              "200": {
                "description": "Message sent successfully"
              }
            },
            "security": [
              {
                "basicAuth": []
              }
            ]
          }
        }
      },
      "components": {
        "securitySchemes": {
          "basicAuth": {
            "type": "http",
            "scheme": "basic",
            "description": "Basic authentication with Crisp API credentials"
          }
        }
      }
    },
    "securityOption": "basicAuth",
    "securitySecrets": {
      "x-secret-username": "username",
      "x-secret-password": "password"
    }
  },
