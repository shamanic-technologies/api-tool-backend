  {
    "id": "chargebee_list_refunds",
    "utilityProvider": "chargebee",
    "openapiSpecification": {
      "openapi": "3.0.0",
      "info": {
        "title": "Chargebee List Refunds",
        "description": "Returns a list of refund transactions from Chargebee",
        "version": "v1"
      },
      "servers": [
        {
          "url": "https://{site}.chargebee.com",
          "variables": {
            "site": {
              "description": "Your Chargebee site name",
              "default": "your-site"
            }
          }
        }
      ],
      "components": {
        "securitySchemes": {
          "basicAuth": {
            "type": "http",
            "scheme": "basic",
            "description": "Basic authentication using your Chargebee API key as username and empty password"
          }
        }
      },
      "paths": {
        "/api/v1/transactions": {
          "get": {
            "operationId": "listRefunds",
            "summary": "List Chargebee refund transactions",
            "description": "Returns a list of refund transactions from Chargebee",
            "parameters": [
              {
                "name": "limit",
                "in": "query",
                "description": "Number of records to fetch",
                "schema": {
                  "type": "integer",
                  "default": 10,
                  "maximum": 100
                }
              },
              {
                "name": "type[is]",
                "in": "query",
                "description": "Filter transactions by type",
                "schema": {
                  "type": "string",
                  "enum": [
                    "refund"
                  ]
                },
                "required": true
              }
            ],
            "security": [
              {
                "basicAuth": []
              }
            ],
            "responses": {
              "200": {
                "description": "List of refund transactions",
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "list": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "transaction": {
                                "type": "object"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "securityOption": "basicAuth",
    "securitySecrets": {
      "x-secret-username": "api_secret_key"
    }
  },
