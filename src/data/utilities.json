  {
    "id": "stripe_list_refunds",
    "securityOption": "stripeBasicAuth",
    "securitySecrets": {
      "x-secret-username": "api_secret_key"
    },
    "utilityProvider": "stripe",
    "openapiSpecification": {
      "servers": [
        {
          "url": "https://api.stripe.com"
        }
      ],
      "info": {
        "title": "Stripe List Refunds",
        "version": "v1",
        "description": "Lists all refunds in your Stripe account"
      },
      "paths": {
        "/v1/refunds": {
          "get": {
            "summary": "List all refunds",
            "security": [
              {
                "stripeBasicAuth": []
              }
            ],
            "responses": {
              "200": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "url": {
                          "type": "string"
                        },
                        "data": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string"
                              },
                              "amount": {
                                "type": "integer"
                              },
                              "charge": {
                                "type": "string"
                              },
                              "object": {
                                "enum": [
                                  "refund"
                                ],
                                "type": "string"
                              },
                              "reason": {
                                "type": "string"
                              },
                              "status": {
                                "type": "string"
                              },
                              "created": {
                                "type": "integer"
                              },
                              "currency": {
                                "type": "string"
                              }
                            }
                          }
                        },
                        "object": {
                          "enum": [
                            "list"
                          ],
                          "type": "string"
                        },
                        "has_more": {
                          "type": "boolean"
                        }
                      }
                    }
                  }
                },
                "description": "A list of refunds"
              }
            },
            "parameters": [
              {
                "in": "query",
                "name": "limit",
                "schema": {
                  "type": "integer",
                  "default": 10,
                  "maximum": 100,
                  "minimum": 1
                },
                "required": false,
                "description": "A limit on the number of objects to be returned, between 1 and 100."
              },
              {
                "in": "query",
                "name": "starting_after",
                "schema": {
                  "type": "string"
                },
                "required": false,
                "description": "A cursor for use in pagination. starting_after is an object ID that defines your place in the list."
              },
              {
                "in": "query",
                "name": "ending_before",
                "schema": {
                  "type": "string"
                },
                "required": false,
                "description": "A cursor for use in pagination. ending_before is an object ID that defines your place in the list."
              }
            ],
            "description": "Returns a list of all refunds you've previously created. The refunds are returned in sorted order, with the most recent refunds appearing first.",
            "operationId": "stripeListRefunds"
          }
        }
      },
      "openapi": "3.0.0",
      "components": {
        "securitySchemes": {
          "stripeBasicAuth": {
            "type": "http",
            "scheme": "basic",
            "description": "HTTP Basic Authentication with Stripe API key as username and empty password."
          }
        }
      }
    }
  },
