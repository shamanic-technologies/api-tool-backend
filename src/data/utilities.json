[
  {
    "id": "crisp_send_message",
    "utilityProvider": "crisp",
    "description": "Envoie un message texte à une conversation Crisp existante",
    "schema": {
      "type": "object",
      "properties": {
        "website_id": {
          "type": "string",
          "description": "L'identifiant du site web Crisp",
          "examples": [
            "8c842203-7ed8-4e29-a608-7cf78a7d2fcc"
          ]
        },
        "session_id": {
          "type": "string",
          "description": "L'identifiant de la session/conversation",
          "examples": [
            "session_700c65e1-85e2-465a-b9ac-ecb5ec2c9881"
          ]
        },
        "message": {
          "type": "string",
          "description": "Le contenu du message texte à envoyer",
          "examples": [
            "Merci pour votre message. Je vous réponds dès que possible."
          ]
        }
      }
    },
    "authMethod": "API_KEY",
    "requiredSecrets": [
      "api_secret_key",
      "api_identifier"
    ],
    "apiKeyDetails": {
      "secretName": "api_secret_key",
      "scheme": "BasicUser"
    },
    "apiDetails": {
      "method": "POST",
      "baseUrl": "https://api.crisp.chat/v1",
      "pathTemplate": "/website/{website_id}/conversation/{session_id}/message",
      "paramMappings": {
        "path": {
          "website_id": "website_id",
          "session_id": "session_id"
        },
        "body": {
          "message": "content"
        }
      },
      "staticHeaders": {
        "Content-Type": "application/json",
        "X-Crisp-Tier": "plugin"
      }
    }
  },
