  {
    "id": "chargebee_get_refunds",
    "utilityProvider": "chargebee",
    "openapiSpecification": {
      "openapi": "3.0.0",
      "info": {
        "title": "Chargebee Refunds",
        "version": "1.0.0",
        "description": "API to get refunds from Chargebee"
      },
      "servers": [
        {
          "url": "https://{site_name}.chargebee.com",
          "variables": {
            "site_name": {
              "default": "your-site",
              "description": "Your Chargebee site name"
            }
          }
        }
      ],
      "paths": {
        "/api/v2/credit_notes": {
          "get": {
            "operationId": "listCreditNotes",
            "summary": "List credit notes",
            "parameters": [
              {
                "in": "query",
                "name": "limit",
                "schema": {
                  "type": "integer",
                  "default": 10
                },
                "description": "Number of records to return"
              },
              {
                "in": "query",
                "name": "status",
                "schema": {
                  "type": "string",
                  "enum": [
                    "refunded"
                  ]
                },
                "description": "Filter by refunded status"
              }
            ],
            "security": [
              {
                "chargebeeBasicAuth": []
              }
            ],
            "responses": {
              "200": {
                "description": "Successful response",
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "list": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "credit_note": {
                                "type": "object",
                                "properties": {
                                  "id": {
                                    "type": "string"
                                  },
                                  "customer_id": {
                                    "type": "string"
                                  },
                                  "subscription_id": {
                                    "type": "string"
                                  },
                                  "reference_invoice_id": {
                                    "type": "string"
                                  },
                                  "type": {
                                    "type": "string"
                                  },
                                  "status": {
                                    "type": "string"
                                  },
                                  "amount_refunded": {
                                    "type": "integer"
                                  },
                                  "refunded_at": {
                                    "type": "integer"
                                  },
                                  "created_at": {
                                    "type": "integer"
                                  },
                                  "currency_code": {
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "components": {
        "securitySchemes": {
          "chargebeeBasicAuth": {
            "type": "http",
            "scheme": "basic",
            "description": "Your Chargebee API key as the username with no password"
          }
        }
      }
    },
    "securityOption": "chargebeeBasicAuth",
    "securitySecrets": {
      "x-secret-username": "api_secret_key"
    }
  },
